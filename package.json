{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "naver-search",
  "title": "Naver Search",
  "description": "Naver typeahead search",
  "icon": "naver.png",
  "author": "eunchurn",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "네이버 검색",
      "description": "Template for showing typeahead search with network results.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.5",
    "@raycast/utils": "^1.4.8",
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}